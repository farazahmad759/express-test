{
  "name": "v1",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon index.js",
    "frontend": "cd frontend && npm run start",
    "build": "cd frontend && npm install && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run frontend\"",
    "git:merge-dev-into-main": "git checkout main && git merge dev && git push origin && git checkout dev",
    "mig": "npx sequelize-cli db:migrate:undo:all & npx sequelize-cli db:migrate & npx sequelize-cli db:seed:undo:all",
    "migseed": "npx sequelize-cli db:migrate:undo:all & npx sequelize-cli db:migrate & npx sequelize-cli db:seed:undo:all & npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.0",
    "hbs": "^4.1.1",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "mariadb": "^2.4.0",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.6",
    "objection": "^2.2.3",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "sequelize-hierarchy": "^2.0.4",
    "update": "^0.7.4"
  }
}
